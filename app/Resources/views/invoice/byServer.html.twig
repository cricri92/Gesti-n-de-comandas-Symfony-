<div class="table-responsive">
    <table class="table table-striped">
        <thead>
            <tr>
                <th>Nombre</th>
                <th># Mesas</th>
                <th># Facturas</th>
                <th>Total</th>
            </tr>
        </thead>
        <tbody>
        {% for row in rows %}
            <tr>
                <td>{{ row.server_name }}</td>
                <td>{{ row.tables }}</td>
                <td>{{ row.invoices }}</td>
                <td>{{ row.total }}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
</div>
